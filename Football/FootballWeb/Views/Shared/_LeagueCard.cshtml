@model ListLeagueResponse
<div class="card">
    <img src=@Model.ImageUrl class="card-img-top" alt="...">
    <div class="card-body">
        <h5 class="card-title">@Model.Name</h5>
        <p class="card-text">@Model.Country</p>
        <a asp-controller="Leagues" asp-action="Details" asp-route-id="@Model.Id" class="btn btn-primary">Detay</a>
    </div>
</div>
